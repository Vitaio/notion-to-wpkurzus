{
  "rewrites": [
    {
      "source": "/api/notion-csv.csv",
      "destination": "/api/notion-csv"
    },
    {
      "source": "/api/notion-csv-plain.csv",
      "destination": "/api/notion-csv"
    }
  ],
  "headers": [
    {
      "source": "/api/notion-csv.csv",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/csv; charset=utf-8"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=lessons.csv"
        },
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    },
    {
      "source": "/api/notion-csv-plain.csv",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/csv; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}